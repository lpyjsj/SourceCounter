<?xml version="1.0" encoding="utf-8"?>
<rules>
  <rule>
    <extension>
      <ruletype>Shell</ruletype>
      <desc>Properties file.</desc>
      <name>.properties</name>
    </extension>
    <extension>
      <ruletype>Shell</ruletype>
      <desc>Shell file.</desc>
      <name>.sh</name>
    </extension>
    <multi-line-comment-end></multi-line-comment-end>
    <multi-line-comment-begin></multi-line-comment-begin>
    <single-line-comment>#</single-line-comment>
    <desc>Shell file style only use # for single line comment.</desc>
    <type>Shell</type>
  </rule>
  <rule>
    <extension>
      <ruletype>PHP</ruletype>
      <desc>PHP file.</desc>
      <name>.php3</name>
    </extension>
    <extension>
      <ruletype>PHP</ruletype>
      <desc>PHP file.</desc>
      <name>.php</name>
    </extension>
    <multi-line-comment-end>*/</multi-line-comment-end>
    <multi-line-comment-begin>/*</multi-line-comment-begin>
    <single-line-comment>//</single-line-comment>
    <desc>PHP source style</desc>
    <type>PHP</type>
  </rule>
  <rule>
    <extension>
      <ruletype>C</ruletype>
      <desc>C++ source file.</desc>
      <name>.cxx</name>
    </extension>
    <extension>
      <ruletype>C</ruletype>
      <desc>C++ header file.</desc>
      <name>.hpp</name>
    </extension>
    <extension>
      <ruletype>C</ruletype>
      <desc>Tlh file.</desc>
      <name>.tlh</name>
    </extension>
    <extension>
      <ruletype>C</ruletype>
      <desc>C source file.</desc>
      <name>.cc</name>
    </extension>
    <extension>
      <ruletype>C</ruletype>
      <desc>C header file.</desc>
      <name>.hh</name>
    </extension>
    <extension>
      <ruletype>C</ruletype>
      <desc>Tli file.</desc>
      <name>.tli</name>
    </extension>
    <extension>
      <ruletype>C</ruletype>
      <desc>C source file.</desc>
      <name>.c</name>
    </extension>
    <extension>
      <ruletype>C</ruletype>
      <desc>JAVA source file.</desc>
      <name>.java</name>
    </extension>
    <extension>
      <ruletype>C</ruletype>
      <desc>C/C++ source file.</desc>
      <name>.cpp</name>
    </extension>
    <extension>
      <ruletype>C</ruletype>
      <desc>C# source file.</desc>
      <name>.cs</name>
    </extension>
    <extension>
      <ruletype>C</ruletype>
      <desc>C/C++ header file.</desc>
      <name>.h</name>
    </extension>
    <multi-line-comment-end>*/</multi-line-comment-end>
    <multi-line-comment-begin>/*</multi-line-comment-begin>
    <single-line-comment>//</single-line-comment>
    <desc>C/C++/JAVA/C# source style</desc>
    <type>C</type>
  </rule>
  <rule>
    <extension>
      <ruletype>Xml</ruletype>
      <desc>HTML file.</desc>
      <name>.html</name>
    </extension>
    <extension>
      <ruletype>Xml</ruletype>
      <desc>Html file.</desc>
      <name>.htm</name>
    </extension>
    <extension>
      <ruletype>Xml</ruletype>
      <desc>XML file.</desc>
      <name>.xml</name>
    </extension>
    <multi-line-comment-end>--&gt;</multi-line-comment-end>
    <multi-line-comment-begin>&lt;!--</multi-line-comment-begin>
    <single-line-comment></single-line-comment>
    <desc>Xml file style</desc>
    <type>Xml</type>
  </rule>
  <rule>
    <extension>
      <ruletype>Sql</ruletype>
      <desc>SQL file.</desc>
      <name>.sql</name>
    </extension>
    <multi-line-comment-end>*/</multi-line-comment-end>
    <multi-line-comment-begin>/*</multi-line-comment-begin>
    <single-line-comment>--</single-line-comment>
    <desc>Sql file style</desc>
    <type>Sql</type>
  </rule>
  <rule>
    <extension>
      <ruletype>Text</ruletype>
      <desc>txt</desc>
      <name>.txt</name>
    </extension>
    <extension>
      <ruletype>Text</ruletype>
      <desc>Resource file.</desc>
      <name>.rc</name>
    </extension>
    <extension>
      <ruletype>Text</ruletype>
      <desc>Delphi form define file.</desc>
      <name>.dfm</name>
    </extension>
    <multi-line-comment-end></multi-line-comment-end>
    <multi-line-comment-begin></multi-line-comment-begin>
    <single-line-comment></single-line-comment>
    <desc>Text file style</desc>
    <type>Text</type>
  </rule>
  <rule>
    <extension>
      <ruletype>JSP</ruletype>
      <desc>JSP file.</desc>
      <name>.jsp</name>
    </extension>
    <multi-line-comment-end>*/</multi-line-comment-end>
    <multi-line-comment-begin>/*</multi-line-comment-begin>
    <single-line-comment>//</single-line-comment>
    <desc>JSP source style.</desc>
    <type>JSP</type>
  </rule>
  <rule>
    <extension>
      <ruletype>ASPX</ruletype>
      <desc>Aspx file.</desc>
      <name>.aspx</name>
    </extension>
    <extension>
      <ruletype>ASPX</ruletype>
      <desc>Asp file.</desc>
      <name>.asp</name>
    </extension>
    <extension>
      <ruletype>ASPX</ruletype>
      <desc>ASP inc file source.</desc>
      <name>.inc</name>
    </extension>
    <multi-line-comment-end></multi-line-comment-end>
    <multi-line-comment-begin></multi-line-comment-begin>
    <single-line-comment>'</single-line-comment>
    <desc>ASPX source style</desc>
    <type>ASPX</type>
  </rule>
  <rule>
    <extension>
      <ruletype>Pascal</ruletype>
      <desc>Delphi pascal source file.</desc>
      <name>.pas</name>
    </extension>
    <multi-line-comment-end>}</multi-line-comment-end>
    <multi-line-comment-begin>{</multi-line-comment-begin>
    <single-line-comment>//</single-line-comment>
    <desc>Pascal source style</desc>
    <type>Pascal</type>
  </rule>
  <rule>
    <extension>
      <ruletype>Basic</ruletype>
      <desc>Frm file.</desc>
      <name>.frm</name>
    </extension>
    <extension>
      <ruletype>Basic</ruletype>
      <desc>Ctl file.</desc>
      <name>.ctl</name>
    </extension>
    <extension>
      <ruletype>Basic</ruletype>
      <desc>Cls file.</desc>
      <name>.cls</name>
    </extension>
    <extension>
      <ruletype>Basic</ruletype>
      <desc>Visual Basic source file.</desc>
      <name>.vb</name>
    </extension>
    <multi-line-comment-end></multi-line-comment-end>
    <multi-line-comment-begin></multi-line-comment-begin>
    <single-line-comment>'</single-line-comment>
    <desc>Basic source style</desc>
    <type>Basic</type>
  </rule>
</rules>
